{
  "openapi": "3.0.0",
  "info": {
    "title": "Users and Cars API",
    "version": "1.0.0",
    "description": "API for managing users and cars"
  },
  "servers": [
    {
      "url": "http://localhost:8000"
    }
  ],
  "paths": {
    "/users/register": {
      "post": {
        "summary": "Register User",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "types": "object",
                "properties": {
                  "username": {
                    "type": "string",
                    "example": "superadmin"
                  },
                  "email": {
                    "type": "string",
                    "example": "superadmin@superadmin.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "superadmin"
                  },
                  "role": {
                    "type": "string",
                    "example": "superadmin"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Register User",
            "content": {
              "application/json": {
                "schema": {
                  "types": "object",
                  "properties": {
                    "status": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "Register User Successfully"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "example": "ed52f847-0c53-428c-8f34-b01cfd4d2635"
                          },
                          "username": {
                            "type": "string",
                            "example": "superadmin"
                          },
                          "email": {
                            "type": "string",
                            "example": "superadmin@superadmin.com"
                          },
                          "password": {
                            "type": "string",
                            "example": "superadmin"
                          },
                          "role": {
                            "type": "string",
                            "example": "admin"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Error message",
            "content": {
              "application/json": {
                "schema": {
                  "types": "object",
                  "properties": {
                    "status": {
                      "type": "boolean",
                      "example": false
                    },
                    "message": {
                      "type": "string",
                      "example": "Access forbidden"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error message",
            "content": {
              "application/json": {
                "schema": {
                  "types": "object",
                  "properties": {
                    "status": {
                      "type": "boolean",
                      "example": false
                    },
                    "message": {
                      "type": "string",
                      "example": "Internal server error"
                    }
                  }
                }
              }
            }
          }
        }
